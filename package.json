{
	"name": "aikeys",
	"displayName": "AI-Keys",
	"description": "Query different generative AI models from varied providers.",
	"icon": "aikeys-icon.png",
	"repository": {
		"url": "https://github.com/tomcodedthis/AI-Keys"
	  },
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Machine Learning",
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "aikeys.goToSettings",
				"title": "AI-Keys: Open Settings"
			},
			{
				"command": "aikeys.sendLinePrompt",
				"title": "AI-Keys: Search Line"
			},
			{
				"command": "aikeys.sendBoxPrompt",
				"title": "AI-Keys: Search Prompt"
			}
		],
		"configuration": [
			{
				"title": "AI-Keys",
				"properties": {
					"AI-Keys.keys": {
						"type": "object",
						"description": "Define your API Keys",
						"properties": {
							"openai": {
								"type": "string"
							}
						},
						"default": {
							"openai": ""
						}
					},
					"AI-Keys.model": {
						"type": "string",
						"description": "Choose default your AI Model",
						"default": "GBT3",
						"enum": [
							"GBT3",
							"Codex",
							"DALLE"
						],
						"enumDescriptions": [
							"Low-Cost: Text-generation from OpenAI",
							"Free: Code-generation from OpenAI",
							"Low-Cost: Image-generation from OpenAI"
						]
					},
					"AI-Keys.maxTokens": {
						"type": "number",
						"description": "Set your max token response (1 token = 1 character)",
						"default": 100
					},
					"AI-Keys.prefix": {
						"type": "string",
						"description": "Add an optional preffix to start your prompt",
						"default": ""
					},
					"AI-Keys.imageFormat": {
						"type": "string",
						"description": "Choose the format of AI image generated.\n\nWINDOW NEEDS TO BE REFRESED TO TAKE EFFECT",
						"default": "webp",
						"enum": [
							"webp",
							"jpg",
							"png"
						],
						"enumDescriptions": [
							"webp",
							"jpg",
							"png"
						]
					}
				}
			}
		],
		"keybindings": [
			{
				"command": "aikeys.sendLinePrompt",
				"key": "ctrl+alt+/",
				"mac": "cmd+alt+/",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"format": "prettier-eslint \"src/**/*.ts\""
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.34.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"prettier-eslint": "^15.0.1",
		"prettier-eslint-cli": "^7.1.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"node-fetch": "^3.3.0",
		"openai": "^3.1.0",
		"title-case": "^3.0.3"
	}
}
